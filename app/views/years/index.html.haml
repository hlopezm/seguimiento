%h1 Listing years

%table
  %thead
    %tr
      %th Nombre

  %tbody
    - @years.each do |year|
      %tr
        %td= link_to year.nombre, year
        %td= link_to 'Edit', edit_year_path(year)
        %td= link_to 'Destroy', year, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Year', new_year_path, class: "btn btn-primary"
